{
  "manifest_version": 2,
  "name": "Messenger Popout",
  "version": "1.0.0",
  "version_name": "1.0.0 beta",
  "author": "Alexander Hofbauer",
  "description": "Framing Facebook Messenger for PopUp use.",
  "icons": {
    "16": "img/icon_16.png",
    "24": "img/icon_24.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": "img/icon_256.png",
    "default_title": "Open Messenger"
  },
  "minimum_chrome_version": "40",
  "offline_enabled": false,
  "options_ui": {
    "page": "html/options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage"
  ]
}
